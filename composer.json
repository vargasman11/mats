{
  "require": {
    "craftcms/cms": "3.4.14",
    "craftcms/feed-me": "4.2.2",
    "craftcms/redactor": "2.6.1",
    "doublesecretagency/craft-smartmap": "3.2.2.1",
    "hashtagerrors/craft-pagination": "1.0.2",
    "solspace/craft-freeform": "3.6.6",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
